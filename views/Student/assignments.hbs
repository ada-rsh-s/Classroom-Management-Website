<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<meta name="description" content="">
	<meta name="author" content="">

	<link href="../Studtable/style.bundled1cf.css" rel="stylesheet" type="text/css" />

	<link rel="stylesheet" type="text/css" href="../Studtemplate/assets/css/style.css">
	<link rel="stylesheet" href="../css/style.css">
</head>

<body id="bg">

	<div class="page-wraper">
		<div class="page-content bg-white">
			<div class="page-banner ovbl-dark"
				style="background-image:url(../Studtemplate/assets/images/banner/banner3.jpg);">
				<div class="container">
					<div class="page-banner-entry">
						<h1 class="text-white">ASSIGNMENTS</h1>
					</div>
				</div>
			</div>
			<div class="d-flex flex-column flex-root">

				<div class="d-flex flex-row flex-column-fluid page">

					<div class="d-flex flex-column flex-row-fluid" id="kt_wrapper">

						<div class="content d-flex flex-column flex-column-fluid" id="kt_content">

							<div class="d-flex flex-column-fluid">
								<!--begin::Container-->

								<div class="container">



									<div class="card card-custom">
										<div class="card-body">
											<!--begin: Datatable-->
											<table id="assigntable" class=" table-bordered table-hover table-checkable "
												style="width: 100%; table-layout: fixed;">
												<thead style="line-height: 50px;">

													<tr>
														<th>
															<center>Date</center>
														</th>
														<th>
															<center>Topic</center>
														</th>
														<th>
															<center>File</center>
														</th>

													</tr>
												</thead>
												<tbody>
													{{#each assign}}
													<tr>
														<td>
															<center>{{this.Topic.Date}}</center>
														</td>
														<td>
															<center>{{this.Topic.Topic}}</center>
														</td>
														<td>
															<center><a href="../Assignments/{{this._id}}.pdf ">
																	<center><img style="height: 40px; width: auto;"
																			src="../Notes/open-document.png"
																			alt="W3Schools" width="104"
																			height="142"><br><small
																			style="color: rgb(17, 165, 17);">Click to
																			download the file</small></center>
															</center>

															<center> <a href="/assignments/{{this._id}}"><button
																		class="btn btn-success">Submit</button></a>
															</center>
														</td>
														<td hidden nowrap="nowrap"></td>
													</tr>
													{{/each}}
												</tbody>
											</table>
											<center>
												<div class="page-banner-entry">
													<h1 style="margin-top: 70px;">SUBMITTED ASSIGNMENTS</h1>
												</div>
											</center>
											<table class=" table-bordered table-hover table-checkable "
												style="width: 100%; table-layout: fixed;">
												<thead style="line-height: 50px;">
													<tr>

														<th>
															<center>Topic</center>
														</th>

														<th>
															<center>Marks</center>
														</th>
													</tr>
												</thead>
												<tbody>
													{{#each submarks}}
													<tr>
														<td>
															<center>{{this.topic}}</center>
														</td>

														<td>
															<center>{{this.mark}} / 20</center>
														</td>
														<td hidden nowrap="nowrap"></td>
													</tr>
													{{/each}}
												</tbody>
											</table>
										</div>
									</div>
								</div>
							</div>
						</div>

					</div>
				</div>
			</div>
		</div>
	</div>

</body>
<script src="../Template/js/vendors.min.js"></script>
<script src="../Template/corenav-master/coreNavigation-1.1.3.js"></script>
<script src="../Template/vendor_components/jquery-ui/jquery-ui.min.js"></script>