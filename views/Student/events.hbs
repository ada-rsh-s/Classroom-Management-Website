<head>

	<link rel="stylesheet" type="text/css" href="../Studtemplate/assets/vendors/revolution/css/layers.css">
	<link href="../A&E/css/base.css" rel="stylesheet" type="text/css" media="all" />
	<link href="../A&E/css/main.css" rel="stylesheet" type="text/css" media="all" />


</head>

<body id="bg">
	<div class="page-wraper">
		<div class="page-content ">
			<div class="page-banner ovbl-dark"
				style="background-image:url(../Studtemplate/assets/images/banner/banner3.jpg);">
				<div class="container">
					<div class="page-banner-entry">
						<h1 class="text-white">EVENTS</h1>
					</div>
				</div>
			</div>
		</div>
		<section id="schedule" class="schedule pt-120 pb-120">
			<!--Container-->
			<div class="container">
				<!--Row-->
				<div class="row">
					<div class="col-sm-12 col-sm-offset-2 mb-100 text-center">
						<h3 class="sub-title-0  mb-25"><span class="gradient-text">EVENTS</span></h3>
					</div>
				</div>
				<!--End row-->
			</div>
			<div style="margin-top: -70px;" class="container">
				<!--Row-->
				<div class="row">
					<div class="col-sm-4 ">
						<!--Tabs-->

					</div>
					<div id="tabb" class="col-sm-12">

						<ul class="block-tab">
							<!--Tab-->
							<li class="active ">
								<center>
									<div class="block-date"><img style="margin-top: -5px;"
											src="../Template/icons/themify-icons/SVG/calendar.svg" alt=""
											srcset=""><strong>&nbsp; EVENTS</strong></div>
								</center>
								{{#each events}}
								<div class="col-sm-12 col-sm-offset-2 mt-20">
									<h1 class="sub-title-0 "><span style="font-size: 17px;"
											class="gradient-text">{{this.Date}}</span></h1>
								</div>
								<div class="block-detail" style="padding-bottom: 50px;">
									<a href="/event/{{this._id}}" style="font-size: 15px; color: black;"
										class="">{{this.Name}}</a>
									<div style="font-weight: bolder;" class="block-text">
										<strong style="color: black;">Speaker</strong> : <a class="gradient-text ">
											{{this.Person}}</a>
									</div>
								</div>
								{{/each}}
							</li>
						</ul>
					</div>
				</div>
				<!--End row-->
			</div>
			<!--End container-->
		</section>
	</div>
	</div>
</body>
<script src="../A&E/js/jquery-1.12.4.min.js" type="text/javascript"></script>
<script src="../A&E/js/jquery.flexslider-min.js" type="text/javascript"></script>
<script src="../A&E/js/jquery.countdown.min.js" type="text/javascript"></script>
<script src="../A&E/js/smooth-scroll.js" type="text/javascript"></script>
<script src="../A&E/js/script.js" type="text/javascript"></script>